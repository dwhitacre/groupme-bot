{
  "name": "groupme-bot",
  "version": "1.0.0",
  "description": "typescript hapi server for groupme chat bot",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "test-nostart": "ts-mocha test.ts",
    "test": "start-server-and-test start 3000/health test-nostart",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "start": "npm run serve",
    "build": "npm run clean-ts && npm run build-ts && npm run lint",
    "serve": "node dist/index.js",
    "watch-node": "nodemon dist/index.js",
    "watch": "npm run build && concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "clean-ts": "rm -rf dist",
    "ngrok": "ts-node ngrok.ts"
  },
  "keywords": [
    "typescript",
    "hapi",
    "groupme",
    "chatbot"
  ],
  "author": "Dan Whitacre",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.2.0",
    "@types/cheerio": "^0.22.13",
    "@types/hapi-pino": "^6.0.0",
    "@types/hapi__hapi": "^18.2.5",
    "@types/hapi__inert": "^5.2.0",
    "@types/hapi__joi": "^15.0.3",
    "@types/hapi__vision": "^5.5.1",
    "@types/hapi__wreck": "^15.0.1",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.7.3",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "chai": "^4.2.0",
    "concurrently": "^4.1.2",
    "eslint": "^6.3.0",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "mocha": "^6.2.0",
    "ngrok": "^3.2.5",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2",
    "start-server-and-test": "^1.10.0",
    "ts-mocha": "^6.0.0",
    "ts-node": "^8.3.0",
    "typescript": "^3.6.2"
  },
  "dependencies": {
    "@hapi/boom": "^7.4.3",
    "@hapi/hapi": "^18.3.2",
    "@hapi/inert": "^5.2.1",
    "@hapi/joi": "^15.1.1",
    "@hapi/vision": "^5.5.3",
    "@hapi/wreck": "^15.0.2",
    "cheerio": "^1.0.0-rc.3",
    "dotenv": "^8.1.0",
    "drive-db": "^5.0.1",
    "hapi-pino": "^6.0.2",
    "hapi-swagger": "^10.1.1"
  },
  "engines": {
    "node": "12"
  }
}
